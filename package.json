{
  "name": "how-muscle-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn install && yarn clear && yarn pm2-start",
    "dev": "yarn install && yarn clear && yarn build && yarn pm2-dev",
    "pm2-start": "yarn build && pm2 start dist/app.js --name 'how-muscle-server'",
    "pm2-dev": "pm2 start ./src/app.js --name 'how-muscle-server' --watch ",
    "kill": "pm2 delete how-muscle-server",
    "build": "babel src -d dist",
    "clear": "rm -rf dist"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pm2": "^5.2.0",
    "pug": "2.0.0-beta11",
    "sequelize": "^6.21.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10"
  }
}
